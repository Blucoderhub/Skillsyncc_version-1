{
  "version": 2,
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node",
      "maxDuration": 30,
      "includeFiles": "dist/**"
    }
  },
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/assets/(.*)",
      "destination": "/dist/public/assets/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}