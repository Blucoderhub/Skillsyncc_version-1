{
  "version": 2,
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node",
      "maxDuration": 30,
      "includeFiles": "dist/**"
    }
  },
  "trailingSlash": false,
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(assets/.*)",
      "destination": "/dist/public/$1"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}