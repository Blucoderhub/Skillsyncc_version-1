{
  "version": 2,
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@5.1.1",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/client/dist/public/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/client/dist/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}